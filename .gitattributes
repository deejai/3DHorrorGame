# Normalize EOL for all files that Git considers text files.
* text=auto eol=lf
3D_Imports/ filter=lfs diff=lfs merge=lfs -text
./3D_Imports filter=lfs diff=lfs merge=lfs -text
3D_Imports filter=lfs diff=lfs merge=lfs -text
3D_Imports/** filter=lfs diff=lfs merge=lfs -text
3D_Imports/**/* filter=lfs diff=lfs merge=lfs -text
*.glb filter=lfs diff=lfs merge=lfs -text
*.gltf filter=lfs diff=lfs merge=lfs -text
